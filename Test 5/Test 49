using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class Test38 : MonoBehaviour
{
    // 두 정수 a, b가 주어질 때 다음과 같은 형태의 계산식을 출력하는 코드를 작성해 보세요. a + b = c

    void Start()
    {
        Solution(4, 3);
        Solution(7, 12);
        Solution(5, 18);
    }

    int Solution(int a, int b)
    {
        int c = 0;

        c = a + b;

        Debug.Log(c);
        return c;
    }
}
