using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class Test1 : MonoBehaviour
{
    // 정수 num1, num2가 매개변수 주어집니다. num1과 num2를 곱한 값을 return 하도록 solution 함수를 완성해주세요.

    void Awake()
    {
        solution(3, 4);
        solution(27, 19);
    }

    int solution(int num1, int num2)
    {
        int answer = 0;
        answer = num1 * num2;
        Debug.Log(answer);
        return answer;
    }
}
