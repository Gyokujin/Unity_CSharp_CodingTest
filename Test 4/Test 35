using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class Test24 : MonoBehaviour
{
    // 1 ~ 10까지 정수 중에서 3으로 나누어 떨어지지 않는 수를 출력하고, 이 수들의 합을 출력

    void Start()
    {
        int sum = 0;

        for (int i = 1; i <= 10; i++)
        {
            if (i % 3 != 0)
            {
                sum += i;
                print(i);
            }
        }

        print(sum);
    }
}
