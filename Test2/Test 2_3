using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class Test2 : MonoBehaviour
{
    // 정수 num1과 num2가 주어질 때, num1에서 num2를 뺀 값을 return하도록 soltuion 함수를 완성해주세요.

    void Awake()
    {
        solution(2, 3);
        solution(100, 2);
    }

    int solution(int num1, int num2)
    {
        int answer = 0;
        answer = num1 - num2;
        Debug.Log(answer);
        return answer;
    }
}
